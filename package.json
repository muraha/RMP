{
  "name": "rmp2020",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "cm": "git-cz",
    "start": "yarn run webpack-dev-server --open --hot --env.dev",
    "build": "yarn run webpack --env.prod",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "check": "yarn generate-css-d-ts && yarn lint:check",
    "lint:fix": "eslint  --fix --ext .jsx,.js,.ts,.tsx .",
    "lint:check": "eslint  --ext .jsx,.js,.ts,.tsx .",
    "generate-css-d-ts": "yarn run tcmi -s -n src/",
    "install": "yarn generate-css-d-ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "formik": "^2.1.6",
    "jest": "^26.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "yarn": "^1.22.4",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/react": "^11.1.1",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.45",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "autoprefixer": "^9.8.5",
    "axios": "^0.20.0",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "deepfreeze": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "git-cz": "4.7.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.2",
    "typed-css-interfaces": "^1.3.2",
