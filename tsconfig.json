{
  "compilerOptions": {
    "outDir": "./dist/",
    "jsx": "react",
    "lib": ["es2015", "dom", "es2017.object", "es2016"],
    "module": "commonjs",
    "noImplicitAny": false,
    "sourceMap": true,
    "target": "es5",
    "keyofStringsOnly": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
  },
  "exclude": ["node_modules"]
}
